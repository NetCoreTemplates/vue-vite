{
  "name": "my-app",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --open",
    "dtos": "cd src && x ts",
    "build": "vue-tsc --noEmit && vite build --emptyOutDir",
    "build:local": "vue-tsc --noEmit && vite build --mode development --emptyOutDir",
    "publish": "npm run build && dotnet publish ../api/MyApp/MyApp.csproj -c Release",
    "serve": "vite preview",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@servicestack/client": "^1.1.9",
    "@vueuse/core": "^7.1.2",
    "pinia": "^2.0.6",
    "swrv": "^1.0.0-beta.8",
    "vue": "^3.2.20",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@iconify-json/emojione": "^1.0.2",
    "@iconify-json/fluent": "^1.0.12",
    "@iconify-json/logos": "^1.0.1",
    "@iconify-json/mdi": "^1.0.10",
    "@iconify-json/noto": "^1.0.3",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/node": "^16.11.7",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vueuse/head": "^0.7.2",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.4",
    "tailwindcss": "^3.0.1",
    "typescript": "4.4",
    "unplugin-auto-import": "^0.5.1",
    "unplugin-icons": "^0.12.20",
    "unplugin-vue-components": "^0.17.2",
    "vite": "^2.6.14",
    "vite-plugin-inspect": "^0.3.11",
    "vite-plugin-md": "^0.11.4",
    "vite-plugin-pages": "^0.19.0-beta.3",
    "vite-plugin-vue-layouts": "^0.5.0",
    "vue-tsc": "^0.29.5"
  }
}
